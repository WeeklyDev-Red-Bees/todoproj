<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link rel='stylesheet' href='/stylesheets/style.css' />
	</head>
	<body>
		<section class="top">
			<div class="container">
				<div class="row">
					<div class="col-xs-12 col-sm-8 col-sm-offset-2 col-md-5 col-md-offset-3">
						<h1><%= title %></h1>
						<p>User Admin</p>
					</div>
				</div>
			</div>
		</section>
		<section class="main">
			<div class="container">
				<div class="row">
					<div class="col-xs-12 col-sm-8 col-sm-offset-2 col-md-5 col-md-offset-3">
						<div class="content">
							<div class="admin-list">
								<ul>
									<li>
										<h3>USERNAME </h3>
										<span class="tasks">NUMBER TASKS</span> 
										<div class="checkbox checkbox-circle">
											<input type="checkbox" id="checkbox<%= i+1 %>">
											<label for="checkbox<%= i+1 %>"></label>
										</div>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<% include ./partials/footer %>
	</body>
</html>